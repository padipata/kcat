<mapper namespace="mapper.HomeworkMapper">
	<select id="getHomework" parameterType="java.lang.String" resultType="model.Homework">
		select * from zuoyefuzhu where category =#{category}
	</select>
	<select id="getCategory"  resultType="model.Homework">
		SELECT DISTINCT category FROM zuoyefuzhu
	</select>
	
	
	
</mapper>