<mapper namespace="mapper.AdminMapper">

	<select id="checkAdmin" parameterType="model.Admin" resultType="model.Admin">
		SELECT id FROM easyui_admin WHERE manager=#{manager} AND password=#{password} LIMIT 1
	</select>
	

