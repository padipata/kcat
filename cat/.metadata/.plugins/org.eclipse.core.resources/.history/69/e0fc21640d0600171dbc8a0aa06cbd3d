<script type="text/javascript">
$(function(){
	var html="";
	$.ajaxSetup({async:false});
	$.post("${pageContext.request.contextPath}/getPalySoft_title.do",function(data){  //循环标题
		$.each(data,function(i,e){	
			html+="<div class='box'><div class='shang'>"+data[i].titleName+"</div><div class='xia' lang='"+data[i].titleName+"'>";
			$.post("${pageContext.request.contextPath}/getPalySoft.do",{'soft_to_title':data[i].id},function(data_2){ 
				$.each(data_2,function(j,f){
					html+="<div class='ruanjian'><a href='"+data_2[j].softUrl+"' target='_blank'><img class='logo' src='${pageContext.request.contextPath}/images/"+data_2[j].softImage+"' ></img></a><div class='ming'><a href='"+data_2[j].softUrl+"' target='_blank'>"+data_2[j].softName+"</a></div><div class='fenlei'>"+data_2[j].softType+"</div><a href='"+data_2[j].softUrl+"' target='_blank'> <a href='"+data_2[j].softUrl+"' target='_blank'><img class='huoqu' src='${pageContext.request.contextPath}/images/huoqu.jpg'></img></a></a></div>";
				});				
			});
			html+="</div></div>";
		});
		$(".left").append(html);
	});

});
</script>

